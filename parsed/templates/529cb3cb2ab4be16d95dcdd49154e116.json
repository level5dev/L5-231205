{"components/custom/schematic/schematic-item-list":"<div class=\"grid-item\" style=\"justify-content:center;\">{{index}}</div>\n<div class=\"grid-item\">{{sku}}</div>\n<div class=\"grid-item\">{{name}}</div>\n{{#if price '==' '0'}}\n    <div class=\"grid-item\"></div>\n    <div class=\"grid-item js-card-atc\" style=\"justify-content: end;\">\n        <button class=\"button sch-btn-disabled\" disabled>{{lang 'unavailable.text'}}</button>\n    </div>\n{{else}}\n    <div id=\"\" class=\"grid-item\">${{price}}</div>\n    \n<div class=\"grid-item js-card-atc\">\n    <div class=\"card-atc__section card-atc__section--qty\">\n        <label for=\"card-atc__qty-{{id}}\" class=\"card-atc__label is-srOnly\">Quantity:</label>\n        <div class=\"card-atc-increment--has-buttons js-card-atc-increment\">\n            <div class=\"form-increment incr-mob\">\n                <button class=\"button button--icon\" data-action=\"dec\" type=\"button\" tabindex=\"0\">\n                    <span class=\"icon-wrapper\" aria-hidden=\"true\">\n                        <svg class=\"icon\">\n                            <use xlink:href=\"#icon-keyboard-arrow-down\"></use>\n                        </svg>\n                    </span>\n                </button>\n                <input type=\"tel\" class=\"form-input form-input--incrementTotal js-card-atc__input--total\" name=\"card-atc__qty-{{id}}\" id=\"card-atc__qty-{{id}}\" value=\"1\" min=\"1\" pattern=\"[0-9]*\" aria-live=\"polite\" tabindex=\"0\">\n                <button class=\"button button--icon\" data-action=\"inc\" type=\"button\" tabindex=\"0\">\n                    <span class=\"icon-wrapper\" aria-hidden=\"true\">\n                        <svg class=\"icon\">\n                            <use xlink:href=\"#icon-keyboard-arrow-up\"></use>\n                        </svg>\n                    </span>\n                </button>\n            </div>\n        </div>\n    </div>\n    <div class=\"card-atc__section card-atc__section--action adc-col\">\n        <button type=\"button\" class=\"card-atc__button button button--primary js-card-atc__button adc-btn\" id=\"card-atc__add-{{id}}\" data-default-message=\"Add to Cart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" data-added-message=\"Add to Cart\" value=\"Add to Cart\" data-card-add-to-cart=\"/cart.php?action=add&amp;product_id={{id}}\" data-event-type=\"product-click\" tabindex=\"0\">Add to Cart</button>\n        <button type=\"button\" class=\"button button--primary js-card-atc__button parts s2 w-button adc-btn-mob\" id=\"card-atc__add-{{id}}\" data-default-message=\"+\" data-wait-message=\"…\" data-added-message=\"+\" value=\"+\" data-card-add-to-cart=\"/cart.php?action=add&amp;product_id={{id}}\" data-event-type=\"product-click\" tabindex=\"0\">+</button>\n        <span class=\"product-status-message aria-description--hidden\">Adding to cart… The item has been added</span>\n    </div>\n    <div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\n        <button class=\"modal-close\" type=\"button\" title=\"Close\">\n            <span class=\"aria-description--hidden\">Close</span>\n            <span aria-hidden=\"true\">&#215;</span>\n        </button>\n        <div class=\"modal-content\"></div>\n        <div class=\"loadingOverlay\"></div>\n    </div>\n</div>\n{{/if}}\n\n"}
