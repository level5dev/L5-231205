{"components/common/breadcrumbs-blog":"<nav aria-label=\"Breadcrumb\">\n    <ol class=\"breadcrumbs\" itemscope itemtype=\"https://schema.org/BreadcrumbList\">\n        {{#unless theme_settings.hide_breadcrumbs }}\n            {{#each breadcrumbs}}\n                <li class=\"breadcrumb {{#if @last}}is-active{{/if}} {{#if @first}}blog-post_home-breadcrumb{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\n                    <a class=\"breadcrumb-label\"\n                       itemprop=\"item\"\n                       href=\"{{url}}\"\n                       {{#if @last}}aria-current=\"page\"{{/if}}\n                    >\n                        {{#and (if @last) (if ../page_type '===' 'product')}}\n                            <span itemprop=\"name\" data-product-sku>{{{../product.sku}}}</span>\n                        {{else}}\n                            {{#if @first}}\n                                <img src=\"{{cdn 'assets/img/blog/home-16px.png'}}\"/>\n                            {{else}}\n                                <span itemprop=\"name\">{{name}}</span>\n                            {{/if}}\n                        {{/and}}\n                    </a>\n                    <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\n                </li>\n            {{/each}}\n        {{/unless}}\n    </ol>\n</nav>\n"}
