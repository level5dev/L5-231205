{"components/custom/category-view-toggle":"{{#if theme_settings.its.category_view_toggle}}\n{{inject 'defaultViewType' theme_settings.product_list_display_mode}}\n    <div class=\"category__view-toggle\">\n        <button aria-label=\"Toggle Product List View\" class=\"category__toggle-view js-category__toggle-view {{#if type 'list'}}active-category-view{{/if}}\" data-view-type=\"list\">\n                <span class=\"icon icon--list\">\n                <svg><use xlink:href=\"#icon-list\"></use></svg>\n            </span>\n        </button>\n        <button aria-label=\"Toggle Product Grid View\" class=\"category__toggle-view js-category__toggle-view {{#if type 'grid'}}active-category-view{{/if}}\" data-view-type=\"grid\">\n                <span class=\"icon icon--th\">\n                <svg><use xlink:href=\"#icon-th\"></use></svg>\n            </span>\n        </button>\n    </div>\n{{/if}}\n"}
